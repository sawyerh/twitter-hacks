{
  "name": "functions",
  "description": "Serverless functions for our Firebase project",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "deploy": "firebase deploy --only functions",
    "serve": "npm run build && firebase emulators:start --only functions,pubsub",
    "set-env": "firebase functions:config:get > .runtimeconfig.json",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "logs": "firebase functions:log"
  },
  "dependencies": {
    "@google-cloud/logging": "^7.3.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "firebase-admin": "^8.12.1",
    "firebase-functions": "^3.6.2",
    "luxon": "^1.24.1",
    "twitter": "^1.7.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/luxon": "^1.24.0",
    "firebase-functions-test": "^0.2.1",
    "firebase-tools": "^8.4.1",
    "typescript": "^3.9.3"
  },
  "engines": {
    "node": "16"
  },
  "private": true
}
